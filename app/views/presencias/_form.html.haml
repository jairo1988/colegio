= form_for @presencia do |f|
  -if @presencia.errors.any?
    #error_explanation
      %h2= "#{pluralize(@presencia.errors.count, "error")} prohibited this presencia from being saved:"
      %ul
        - @presencia.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :presentado
    = f.text_field :presentado
  .actions
    = f.submit 'Save'
